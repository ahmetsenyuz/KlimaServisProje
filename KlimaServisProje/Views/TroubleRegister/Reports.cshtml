@model List<TroubleRegisterViewModel>
    <section id="about"  class="text">
		<!-- =============== container =============== -->
        <div class="container">
            <span class="angle2"></span>
            <span class="angle"></span>
            <div class="row">
                <h1 style="color:#ff4c00;
                           margin-bottom:20px;
                           position:relative;
                           font-family: 'Oswald';">
                    @User.Identity.Name Kullanıcısının Raporları
                </h1>
                <table class="table table-striped table-bordered" style="border-width: 3px">
                    <thead>
                    <tr>
                        <th scope="col">#</th>
                        <th scope="col">Klima Markası</th>
                        <th scope="col">Klima Tipi</th>
                        <th scope="col">Kapasitesi(BTU)</th>
                        <th scope="col">Gaz tipi</th>
                        <th scope="col">Arıza Açıklaması</th>
                        <th scope="col">Adres</th>
                        <th scope="col">Ödenme Durumu</th>
                        <th scope="col">İşin Durumu</th>
                    </tr>
                    </thead>
                    <tbody style="font-palette: dark">
                    @foreach (var item in Model)
                    {
                        <tr>
                            <th scope="row">@item.Id</th>
                            <td>@item.ACModel</td>
                            <td>@item.ACType</td>
                            <td>@item.Capacity</td>
                            <td>@item.GasType</td>
                            <td>@item.Description</td>
                            <td>@item.Address</td>
                            @if (item.FeeStatus == true)
                            {
                                <td>
                                    <p class="text text-success">Ödendi</p></td>
                            }
                            else
                            {
                                <td>
                                    <p class="text text-danger">Ödenmedi</p></td>

                            }
                            @if (item.Finished == true)
                            {
                                <td>
                                    <p class="text text-success">Tamamlandı</p></td>
                            }
                            else
                            {
                                <td>
                                    <p class="text text-danger">Tamamlanmadı</p></td>

                            }
                        </tr>
                    }
                    </tbody>
                </table>
            </div>
        </div>   
		<!-- =============== container end =============== -->		
    </section>